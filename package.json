{
  "name": "white-rabbit",
  "productName": "White Rabbit",
  "description": "Alice's Wonderland White Rabbit. Working for family / personal accounting.",
  "author": "Ukonn Ra <ukonnra@outlook.com>",
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@4.5.0",
  "engines": {
    "yarn": ">=4",
    "node": ">=22"
  },
  "scripts": {
    "postinstall": "husky",
    "build": "cargo build --release && yarn build:quasar",
    "build:quasar": "yarn build:buf && quasar build -m ssr && quasar build -m pwa && quasar build -m electron && quasar build",
    "build:buf": "rimraf src-electron/proto/gen; buf generate",
    "lint:rs": "cargo fmt --check && cargo clippy",
    "lint:fix:rs": "cargo sort -w && cargo fmt && cargo clippy --fix --allow-dirty --allow-staged",
    "lint:stylelint": "stylelint '**/*.{vue,scss,css}' --ignore-path .prettierignore",
    "lint:prettier": "prettier '**/*.{vue,ts,js,json}'",
    "lint:eslint": "eslint '**/*.{vue,ts,js,mjs}'",
    "lint:fix": "yarn lint:prettier --write && yarn lint:stylelint --fix && yarn lint:fix:rs && yarn lint:eslint --fix",
    "lint": "buf lint && yarn lint:prettier --check && yarn lint:stylelint && yarn lint:rs && yarn lint:eslint",
    "upgrade": "yarn upgrade-interactive && concurrently 'rimraf .yarn/cache/ node_modules/ yarn.lock && yarn' 'cargo update && cargo upgrade --incompatible --pinned'",
    "test": "cargo test && vitest run",
    "test:coverage": "yarn test:coverage:rs && vitest run --coverage",
    "test:coverage:rs": "rimraf target/debug/coverage; yarn test:coverage:test && yarn test:coverage:generate -t html -o target/debug/coverage/ && yarn test:coverage:generate -t lcov -o target/debug/coverage/lcov.info",
    "test:coverage:test": "CARGO_INCREMENTAL='0' RUSTDOCFLAGS='-Zprofile -Ccodegen-units=1 -Cinline-threshold=0 -Clink-dead-code -Coverflow-checks=off -Cpanic=abort -Zpanic_abort_tests' RUSTFLAGS='-Zprofile -Ccodegen-units=1 -Cinline-threshold=0 -Clink-dead-code -Coverflow-checks=off -Cpanic=abort -Zpanic_abort_tests' cargo +nightly test",
    "test:coverage:generate": "grcov . -s . --binary-path ./target/debug/ --branch --keep-only 'packages/*'",
    "serve": "node './node_modules/@quasar/cli/bin/quasar.js' serve"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.42.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@electron/packager": "^18.3.5",
    "@eslint/js": "^9.11.1",
    "@intlify/unplugin-vue-i18n": "^5.2.0",
    "@quasar/app-vite": "^2.0.0-beta.22",
    "@quasar/cli": "^2.4.1",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.1.0",
    "@quasar/vite-plugin": "^1.7.0",
    "@types/eslint": "^9.6.1",
    "@types/google-protobuf": "^3.15.12",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.9",
    "@types/node": "^22.7.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/coverage-v8": "^2.1.1",
    "@vue/compiler-dom": "^3.5.8",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.0",
    "commitlint": "^19.5.0",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.4.5",
    "electron": "^32.1.2",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.28.0",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "postcss": "^8.4.47",
    "postcss-html": "^1.7.0",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.79.3",
    "sass-loader": "^16.0.2",
    "stylelint": "^16.9.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8",
    "vitest": "^2.1.1",
    "vue": "^3.5.8",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.6",
    "workbox-build": "^7.1.1",
    "workbox-cacheable-response": "^7.1.0",
    "workbox-core": "^7.1.0",
    "workbox-expiration": "^7.1.0",
    "workbox-precaching": "^7.1.0",
    "workbox-routing": "^7.1.0",
    "workbox-strategies": "^7.1.0"
  },
  "dependencies": {
    "@ag-grid-community/client-side-row-model": "^32.2.0",
    "@ag-grid-community/core": "^32.2.0",
    "@ag-grid-community/styles": "^32.2.0",
    "@ag-grid-community/vue3": "^32.2.0",
    "@ag-grid-enterprise/clipboard": "^32.2.0",
    "@ag-grid-enterprise/column-tool-panel": "^32.2.0",
    "@ag-grid-enterprise/core": "^32.2.0",
    "@ag-grid-enterprise/filter-tool-panel": "^32.2.0",
    "@ag-grid-enterprise/menu": "^32.2.0",
    "@ag-grid-enterprise/multi-filter": "^32.2.0",
    "@ag-grid-enterprise/range-selection": "^32.2.0",
    "@ag-grid-enterprise/row-grouping": "^32.2.0",
    "@ag-grid-enterprise/set-filter": "^32.2.0",
    "@bufbuild/protobuf": "^2.1.0",
    "@grpc/grpc-js": "^1.11.3",
    "@quasar/extras": "^1.16.12",
    "@tanstack/vue-query": "^5.56.2",
    "@vueuse/core": "^11.1.0",
    "ag-charts-community": "^10.2.0",
    "date-fns": "^4.1.0",
    "google-protobuf": "^3.21.4",
    "pinia": "^2.2.2",
    "quasar": "^2.17.0",
    "register-service-worker": "^1.7.2",
    "uuid": "^10.0.0",
    "vue": "^3.5.8",
    "vue-i18n": "^10.0.3",
    "vue-router": "^4.4.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
