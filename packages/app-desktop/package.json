    "@white-rabbit/frontend-shared": "workspace:^",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.2.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "eslint": "^8.27.0",
    "vite": "^3.2.3",
    "vitest": "^0.25.1",
    "vue-tsc": "^1.0.9"
  }
}
