    "@white-rabbit/frontend-shared": "workspace:^",
    "vue": "^3.3.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.3.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.40.0",
    "vite": "^4.3.5",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.7.1"
  }
}
