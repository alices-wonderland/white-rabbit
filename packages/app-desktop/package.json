{
  "name": "@white-rabbit/app-desktop",
  "packageManager": "yarn@4.1.1",
  "version": "0.1.0",
  "scripts": {
    "dev": "yarn tauri dev",
    "dev:vue": "vite",
    "test": "vitest run --coverage",
    "lint:tsc": "vue-tsc --noEmit",
    "lint:eslint": "eslint '**/*.{vue,ts,js,mjs}'",
    "lint": "yarn lint:eslint && yarn lint:tsc",
    "lint:fix": "yarn lint:eslint --fix",
    "build": "yarn tauri build",
    "build:vue": "yarn clean && vite build",
    "preview": "vite preview",
    "clean": "rimraf dist/ node_modules/"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0-beta.9",
    "@tauri-apps/plugin-clipboard-manager": "^2.1.0-beta.0",
    "@tauri-apps/plugin-dialog": "^2.0.0-beta.2",
    "@tauri-apps/plugin-fs": "^2.0.0-beta.2",
    "@tauri-apps/plugin-global-shortcut": "^2.0.0-beta.2",
    "@tauri-apps/plugin-http": "^2.0.0-beta.2",
    "@tauri-apps/plugin-notification": "^2.0.0-beta.2",
    "@tauri-apps/plugin-os": "^2.0.0-beta.2",
    "@tauri-apps/plugin-process": "^2.0.0-beta.2",
    "@tauri-apps/plugin-shell": "^2.0.0-beta.2"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0-beta.13",
    "@vitejs/plugin-vue": "^5.0.4",
    "rimraf": "^5.0.5",
    "vite": "^5.2.10",
    "vitest": "^1.5.2",
    "vue-tsc": "^2.0.15"
  }
}
