  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.2.3",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.37.0",
    "vite": "^4.2.1",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.3.8"
  }
}
