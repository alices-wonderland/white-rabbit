    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.1.0",
    "@vueuse/core": "^9.3.0",
    "@white-rabbit/frontend-shared": "workspace:^",
    "vue": "^3.2.40"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.1.1",
    "@vitejs/plugin-vue": "^3.1.2",
    "eslint": "^8.24.0",
    "vite": "^3.1.5",
    "vitest": "^0.23.4",
    "vue-tsc": "^0.40.13"
  }
}
